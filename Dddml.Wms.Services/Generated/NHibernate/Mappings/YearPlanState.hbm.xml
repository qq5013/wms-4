<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
				   assembly="Dddml.Wms.Common" 
				   namespace="Dddml.Wms.Domain">
  
  <class name="Dddml.Wms.Domain.YearPlanState" table="YearPlans">

	<composite-id name="YearPlanId" class="Dddml.Wms.Domain.YearPlanId">
		<key-property name="PersonalNameFirstName" column="YearPlanIdPersonalNameFirstName" length="50"/>
		<key-property name="PersonalNameLastName" column="YearPlanIdPersonalNameLastName" length="50"/>
		<key-property name="Year" column="YearPlanIdYear" />
	</composite-id>

	<version name="Version" type="long" unsaved-value="0"/>

	<property name="Description" length="500"></property>

	<property name="CreatedBy" ></property>

	<property name="CreatedAt" ></property>

	<property name="UpdatedBy" ></property>

	<property name="UpdatedAt" ></property>

	<property name="Active" ></property>

	<property name="Deleted" ></property>


  </class>
  
</hibernate-mapping>

