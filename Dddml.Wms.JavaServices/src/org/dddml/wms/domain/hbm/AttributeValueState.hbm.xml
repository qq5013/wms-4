<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain">
  
  <class name="org.dddml.wms.domain.AbstractAttributeValueState$SimpleAttributeValueState" table="AttributeValues">

	<composite-id name="attributeValueId" class="org.dddml.wms.domain.AttributeValueId">
		<key-property name="attributeId" column="AttributeValueIdAttributeId" length="50"/>
		<key-property name="value" column="AttributeValueIdValue" length="50"/>
	</composite-id>

	<version name="version" type="long" unsaved-value="null"/>

	<property name="name" ></property>

	<property name="description" ></property>

	<property name="referenceId" ></property>

	<property name="createdBy" ></property>

	<property name="createdAt" ></property>

	<property name="updatedBy" ></property>

	<property name="updatedAt" ></property>

	<property name="active" ></property>

	<property name="deleted" ></property>


  </class>
  
</hibernate-mapping>

