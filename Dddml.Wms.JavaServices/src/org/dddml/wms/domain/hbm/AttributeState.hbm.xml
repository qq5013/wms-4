<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain">
  
  <class name="org.dddml.wms.domain.AbstractAttributeState$SimpleAttributeState" table="Attributes">

	<id name="attributeId" length="50">
		<generator class="assigned"/>
	</id>

	<version name="version" type="long" unsaved-value="null"/>

	<property name="name" ></property>

	<property name="organizationId" ></property>

	<property name="description" ></property>

	<property name="isMandatory" ></property>

	<property name="isInstanceAttribute" ></property>

	<property name="attributeValueType" ></property>

	<property name="attributeValueLength" ></property>

	<property name="isList" ></property>

	<property name="fieldName" unique-key="UniqueFieldName"></property>

	<property name="referenceId" ></property>

	<property name="createdBy" ></property>

	<property name="createdAt" ></property>

	<property name="updatedBy" ></property>

	<property name="updatedAt" ></property>

	<property name="active" ></property>

	<property name="deleted" ></property>


  </class>
  
</hibernate-mapping>

