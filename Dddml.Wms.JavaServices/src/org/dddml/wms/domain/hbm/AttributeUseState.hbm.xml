<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.dddml.wms.domain">
  
  <class name="org.dddml.wms.domain.AbstractAttributeUseState$SimpleAttributeUseState" table="AttributeUses">

	<composite-id name="attributeSetAttributeUseId" class="org.dddml.wms.domain.AttributeSetAttributeUseId">
		<key-property name="attributeSetId" column="AttributeSetAttributeUseIdAttributeSetId" length="50"/>
		<key-property name="attributeId" column="AttributeSetAttributeUseIdAttributeId" length="50"/>
	</composite-id>

	<version name="version" type="long" unsaved-value="null"/>

	<property name="sequenceNumber" ></property>

	<property name="createdBy" ></property>

	<property name="createdAt" ></property>

	<property name="updatedBy" ></property>

	<property name="updatedAt" ></property>

	<property name="active" ></property>

	<property name="deleted" ></property>


  </class>
  
</hibernate-mapping>

